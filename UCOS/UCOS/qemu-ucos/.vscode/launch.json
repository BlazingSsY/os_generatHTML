{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "(powerpc-gdb) Launch",
          "type": "cppdbg",
          "request": "launch",
          "program": "${workspaceFolder}/build/bootApp.elf",
          "args": [
              "-i=mi"
          ],
          "stopAtEntry": true,
          "cwd": "${workspaceFolder}",
          "environment": [],
          "externalConsole": false,
          "MIMode": "gdb",
          "setupCommands": [
              {
                  "description": "Enable pretty-printing for gdb-multiarch",
                  "text": "-enable-pretty-printing",
                  "ignoreFailures": false
              }
          ],
         /* "preLaunchTask": "debug",*/
          "miDebuggerPath": "/usr/bin/gdb-multiarch",
          "miDebuggerServerAddress": "192.168.233.129:1234",
          "logging": {
              "engineLogging": true
          }
      }
     
  ]
}